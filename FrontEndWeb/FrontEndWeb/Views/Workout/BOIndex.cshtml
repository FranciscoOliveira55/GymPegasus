@model IEnumerable<FrontEndWeb.Models.Workout>

@{
    ViewData["Title"] = "BOIndex";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Workouts</h1>
<hr />

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ClienteId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HourInit)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HourEnd)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WorkoutPlanId)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ClienteId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.HourInit)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.HourEnd)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.WorkoutPlanId)
                </td>
                <td>
                    @Html.ActionLink("Delete", "BODelete", new { id = item.Id })
                </td>
            </tr>
        }
    </tbody>
</table>
